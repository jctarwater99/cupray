{
  "expo": {
    "name": "cupray",
    "slug": "cupray",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_logo.png",
    "splash": {
      "image": "./assets/app_logo.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.TreetopCreative.CUPray",
      "buildNumber": "1.0.0"
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.testSideLoad.cupray"
    },
    "web": {
      "favicon": "./assets/apple-touch-icon.png"
    }
  }
}
